{
  "version": "1",
  "manifest_version": 3,
  "name": "screen record",
  "background": {
    "service_worker": "background.js"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self' ; object-src 'self'"
  },
  "description": "screen record",
  "permissions": ["desktopCapture"],
  "action": {
    "default_icon": "helloicon.png"
  }
}
